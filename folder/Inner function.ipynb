   "source": [
    "def manipulate(x, y):\n",
    "    # concatenate two strings\n",
    "    def inner_fun(x, y):\n",
    "        return x + y\n",
    "\n",
    "    z = inner_fun(x, y)\n",
    "    return z + 'Shubham'\n",
    "\n",
    "result = manipulate('My', 'Name-')\n",
    "print(result)"
